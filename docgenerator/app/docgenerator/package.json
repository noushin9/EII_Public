{
  "name": "docgenerator",
  "version": "0.0.1",
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "dependencies": {},
  "devDependencies": {
    "@ui5/cli": "^4.0.16",
    "@sap/ux-ui5-tooling": "1"
  },
"scripts": {
  "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
  "build": "ui5 build --dest dist && npx copyfiles xs-app.json dist/",
  "build:cf": "npm run build && cd dist && zip -r ../docgen_app-content.zip * && cd .."
}
} 
