{
  "name": "incident-management",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js/sqlite": "^2.1.2",
    "@sap/cds": "^9",
    "@sap/cds-dk": "9.6.1",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2.1.2"
  },
  "scripts": {
    "start": "cds deploy --to sqlite && cds serve",
    "watch-incidents": "cds watch --open ns.incidents/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-incidentfree": "cds watch --open ns.incidentfree.incidentfree/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "sapux": [
    "app/incidents"
  ],
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "schema_evolution": "auto"
      }
    }
  },
  "engines": {
    "node": ">=20"
  }
}